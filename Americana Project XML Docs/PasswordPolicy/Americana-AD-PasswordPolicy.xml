<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE sailpoint PUBLIC "sailpoint.dtd" "sailpoint.dtd">
<sailpoint>
<PasswordPolicy description="Americana Password Policy" name="Americana-AD-PasswordPolicy">
  <PasswordConstraints>
    <Map>
      <entry key="checkCaseSensitive">
        <value>
          <Boolean />
        </value>
      </entry>
      <entry key="checkPasswordAgainstAccountID">
        <value>
          <Boolean>true</Boolean>
        </value>
      </entry>
      <entry key="checkPasswordAgainstDisplayName">
        <value>
          <Boolean>true</Boolean>
        </value>
      </entry>
      <entry key="checkPasswordTriviality">
        <value>
          <Boolean />
        </value>
      </entry>
      <entry key="checkPasswordsAgainstAccountAttributes">
        <value>
          <Boolean>true</Boolean>
        </value>
      </entry>
      <entry key="checkPasswordsAgainstDictionary">
        <value>
          <Boolean />
        </value>
      </entry>
      <entry key="checkPasswordsAgainstIdentityAttributes">
        <value>
          <Boolean>true</Boolean>
        </value>
      </entry>
      <entry key="minAccountIDUniqueChars" value="3" />
      <entry key="minDisplayNameUniqueChars" value="3" />
      <entry key="minHistoryUniqueChars" />
      <entry key="passwordHistory" value="3" />
      <entry key="passwordMaxLength" />
      <entry key="passwordMinAlpha" />
      <entry key="passwordMinCharType" />
      <entry key="passwordMinLength" value="8" />
      <entry key="passwordMinLower" />
      <entry key="passwordMinNumeric" value="1" />
      <entry key="passwordMinSpecial" value="1" />
      <entry key="passwordMinUpper" value="1" />
      <entry key="passwordRepeatedChar" />
    </Map>
  </PasswordConstraints>
</PasswordPolicy>

</sailpoint>